<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>SubtitleManager</PackageId>
    <Title>Subtitle Manager</Title>
    <Version>1.0.1</Version>
    <Authors>Mirƒça Milanov</Authors>
    <Company>SubtitleManager</Company>
    <Description>
      A .Net Standard library that provides functionality for serialization, parsing/deserialization and editing of subtitles.
      At the moment supported subtitle format is: SubRip (.srt).
    </Description>
    <RepositoryUrl>https://github.com/mikanaaa/Subtitle-Manager</RepositoryUrl>
    <PackageTags>Subtitle, Srt, SubRip, Serialization, Parsing, Video</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

</Project>
